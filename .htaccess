<IfModule mod_deflate.c>
  # Habilitar la compresión de archivos
  SetOutputFilter DEFLATE
</IfModule>
<IfModule mod_expires.c>
  # Cacheo de archivos de imagen, CSS y JavaScript
  ExpiresActive On
  ExpiresByType image/* "access plus 1 month"
  ExpiresByType text/css "access plus 1 month"
  ExpiresByType text/javascript "access plus 1 month"
  ExpiresByType application/javascript "access plus 1 month"
  ExpiresByType application/x-javascript "access plus 1 month"
</IfModule>
<IfModule mod_headers.c>
  # Desactivar ETags para mejorar el rendimiento de la cache
  Header unset ETag
  FileETag None
  # Configuración del cacheo del navegador
  <FilesMatch "\.(jpg|jpeg|png|webp|gif|swf|ico|js|css|ttf|woff2)$">
    Header set Cache-Control "max-age=31536000, public"
  </FilesMatch>
  <Files "index.php">
      Header set Cache-Control "max-age=3600, public"
  </Files>
</IfModule>
<IfModule mod_rewrite.c>
  # Configuración de seguridad
Options +FollowSymLinks
  RewriteEngine On
  # No permitir la navegación por las carpetas del servidor
  # Activar la opción de mostrar errores de PHP en el navegador
  php_flag display_errors on
  # Activar la opción de reportar y mostrar todos los errores de PHP
  php_flag display_startup_errors on
  php_value error_reporting -1
  # Opciones para mejorar la velocidad y eficiencia
  # Denegar acceso si la URL no corresponde a un archivo o carpeta existente
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-f
  # Crear variable en $_GET con la url despues del dominio
  RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
  # Eliminación de las extensiones de archivos .php de la URL
  RewriteCond %{REQUEST_FILENAME}\.php -f
  RewriteRule ^(.*)$ $1.php [NC,L]
</IfModule>
