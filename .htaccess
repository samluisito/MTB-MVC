<IfModule pagespeed_module>
  ModPagespeed on
  # Habilitar filtros de optimización
  ModPagespeedEnableFilters combine_css,combine_javascript,convert_jpeg_to_progressive,resize_images

  # Habilitar caché de recursos
  ModPagespeedFetchHttps enable
  ModPagespeedInheritVHostConfig on
  ModPagespeedRewriteLevel PassThrough

  # Configuraciones adicionales
  ModPagespeedFileCachePath /var/cache/mod_pagespeed/
  ModPagespeedFileCacheInodeLimit 500000
  ModPagespeedFileCacheSizeKb 1024000

  # Configuración de cabeceras de respuesta
  <Location /pagespeed_admin>
    Order allow,deny
    Allow from all
    SetHandler pagespeed_admin
  </Location>

  # Configuración de cabeceras de respuesta para bypassing cache
  <Location /pagespeed_global_admin>
    Order allow,deny
    Allow from all
    SetHandler pagespeed_global_admin
  </Location>
</IfModule>
